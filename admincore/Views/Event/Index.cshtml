@{
    ViewData["Title"] = "Event";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1 style="text-align: center"> Event List</h1>
<div class="row">
    <div class="col-md-12">

        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Table heading</th>
                        <th>Table heading</th>

                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th scope="row">1</th>
                        <td>Serial Number</td>
                        <td><input type="text" class="textbox"></td>


                    </tr>
                    <tr>
                        <th scope="row">2</th>
                        <td>Title of Event</td>
                        <td><input type="text" class="textbox"></td>


                    </tr>

                    <tr>
                        <th scope="row">3</th>
                        <td>Select File</td>
                        <td>
                            <input type="file" class="btn btn-default btn-file" ID="fileuploadimages1" runat="server" /><br>
                        </td>
                    </tr>
                    <tr>
                        <th scope="row">4</th>
                        <td>Select Date</td>
                        <td>
                            <input type="date" class="date" />
                        </td>
                    </tr>
                    <tr>
                        <th scope="row"></th>
                        <td></td>
                        <td>
                            <button class="btn btn-primary" type="submit" style="float: right;">Submit<div></div></button>
                        </td>
                    </tr>

                </tbody>
            </table>


        </div>
    </div>
    <div class="col-md-12">
        <a href="/Event/AddEdit" class="btn btn-success">Add</a>
    </div>
</div>
@section EndScripts{
}
